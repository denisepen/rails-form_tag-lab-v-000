<h1>Student Form</h1>
<%= form_tag students_path do %>
  <label>First Name:</label><br>
  <%= text_field_tag :student, params[:student], id: 'autocomplete' %>
  <%= text_field_tag :'student[first_name]', id: 'autocomplete' %><br>
 
  <label>Last Name:</label><br>
  <%= text_field_tag :'student[last_name]' %><br>
 
  <%= submit_tag "Submit Student" %>
<% end %>

<%= params.inspect %>